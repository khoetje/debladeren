<div class="pagination span-12 mt11">
  <div class="wrapper wrapper-nest edge">
    <div class="span-3 sm-span-3 auto v-center a-left">
      {% if paginate.previous %}
        <a class="paginate-buttons mb0" href="{{ paginate.previous.url }}" >
          {% render 'snip-icons', type: 'theme', icon: 'left-arrow', classes: 'md', fill: 'var(--link-color)', hover: 'var(--link-hover)' %}
        </a>
      {% else %}
        <span class="paginate-buttons disabled mb0">{% render 'snip-icons', type: 'theme', icon: 'left-arrow', classes: 'md', fill: 'var(--link-color)', hover: 'var(--link-hover)' %}</span>
      {% endif%}
    </div>
    <div class="span-6 sm-span-6 auto v-center a-center">
      {% for part in paginate.parts %}
        {% if part.is_link %}
          <a class="page-number mb0" href="{{ part.url }}">{{ part.title }}</a>
        {% else %}
          {% if part.title == '&hellip;' %}
            <span class="page-number mb0">{{ part.title }}</span>
          {% else %}
            <span class="page-number current mb0">{{ part.title }}</span>
          {% endif %}
        {% endif %}
      {% endfor %}
    </div>
    <div class="span-3 sm-span-3 auto v-center a-right">
      {% if paginate.next %}
        <a class="paginate-buttons mb0" href="{{ paginate.next.url }}">
          {% render 'snip-icons', type: 'theme', icon: 'right-arrow', classes: 'md', fill: 'var(--link-color)', hover: 'var(--link-hover)' %}
        </a>
      {% else %}
        <span class="paginate-buttons disabled mb0">{% render 'snip-icons', type: 'theme', icon: 'right-arrow', classes: 'md', fill: 'var(--link-color)', hover: 'var(--link-hover)' %}</span>
      {% endif %}
    </div>
  </div>
</div>
